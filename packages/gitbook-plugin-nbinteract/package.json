{
  "name": "gitbook-plugin-nbinteract",
  "version": "0.0.12",
  "description": "Gitbook plugin for nbinteract",
  "main": "index.js",
  "engines": {
    "gitbook": ">=2.0.0"
  },
  "scripts": {
    "test": "echo \"Error: no tests available\" && exit 1",
    "build": "webpack --config webpack.prod.js",
    "watch": "webpack --watch --config webpack.dev.js",
    "load": "cp ../nbinteract/built/index.bundle.js js/nbinteract.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SamLau95/nb-interact.git"
  },
  "keywords": [
    "jupyter",
    "notebook",
    "html",
    "interactive",
    "widgets",
    "gitbook"
  ],
  "author": "Sam Lau",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/SamLau95/nb-interact/issues"
  },
  "homepage": "https://github.com/SamLau95/nb-interact#readme",
  "dependencies": {
    "nbinteract-core": "^0.0.12"
  }
}
